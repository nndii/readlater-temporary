
{
  "manifest_version": 2,
  "name": "randlater",
  "version": "1.0.2",
  "description": "This is an add-on that allows you to store and pop your 'readlater' articles.",
  "icons": {
    "64": "static/Rust.png"
  },
  "browser_specific_settings": {
    "gecko": {
        "id": "randlater@pm.me"
    }
  },
  "permissions": [
      "activeTab",
      "storage",
      "tabs"
  ],
  "browser_action": {
    "default_title": "randlater",
    "default_popup": "target/deploy/index.html"
  }
}
